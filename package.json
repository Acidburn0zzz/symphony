{
  "name": "IOHK-Orpheus",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "budo src/js/main.js:dist/app.js --live --open",
    "test": "budo src/js/main.js:dist/app.js --live --open -- -t babelify",
    "build": "browserify src/js/main.js | uglifyjs -cm > dist/app.js"
  },
  "browserify": {
    "transform": [
      "babelify",
      "glslify"
    ]
  },
  "author": "Scott Darby <scott@kuva.io>",
  "dependencies": {
    "blockchain.info": "^2.10.1",
    "firebase": "^4.5.0",
    "three": "^0.87.1",
    "three-orbit-controls": "^82.1.0",
    "tsne-js": "^1.0.3",
    "voronoi": "^1.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "budo": "^10.0.3",
    "glslify": "^6.1.0",
    "uglify-js": "^3.0.24"
  }
}
